@model IEnumerable<parakeet.Models.Design>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            @*<th>
                @Html.DisplayNameFor(model => model.DesignArray)
            </th>*@
            <th>
                @Html.DisplayNameFor(model => model.DesignName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Popularitycounter)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Approved)
