                @Html.DisplayFor(modelItem => item.GameTag)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MusicTag)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MovieTag)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CoolTag)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DesignId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DesignId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DesignId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
